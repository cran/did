note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Article",
         title = "Difference-in-differences with multiple time periods",
         author = c(person("Brantly","Callaway"),
                    person("Pedro H.C.", "Sant\'Anna")),
         year = "2020",
         journal = "Forthcoming at the Journal of Econometrics",
         url = "https://arxiv.org/abs/1803.09015",
         header = "To cite the paper that the R did package is based on:"
)


bibentry(
  bibtype  = "Misc",
  title    = "did: Difference in Differences",
  author   = c(person("Brantly", "Callaway"),
               person("Pedro H.C.", "Sant\'Anna")
               ),
  year     = "2020",
  note     = note,
  url      = "https://bcallaway11.github.io/did/",
  header   = "To cite the did R package:"
)
